{
    "manifest_version": 2,
    "name": "Tasky",
    "description": "A todo list to help you conquer your day!",
    "version": "1",
    "author": "Ryan DiCenzo",
   "browser_action": {
      "default_icon": "tab-icon.png",
      "default_title": "Have a produtive day!",
      "default_popup": "index.html"
    },
   "chrome_url_overrides" : {
     "newtab": "index.html"
   },
    "permissions": ["activeTab"]
   }